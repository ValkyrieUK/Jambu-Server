<div id="user_nav">
  <% if user_signed_in? %>
    <p><%= link_to "Logout", destroy_user_session_path, method: :delete %>
    <%= link_to 'Other Users', users_path %> <%= link_to 'My Friends', friends_path %></p>  
    Logged in as <strong><%= current_user.username %></strong>.
    <%= image_tag current_user.image_url %>
  <% else %>
    <%= link_to "Sign in with Twitter", user_omniauth_authorize_path(:twitter) %>
  <% end %>

  <table border="1">
    <% User.all.each do |user| %>
    <tr>
      <td><%= user.username %></td>
      <td><%= image_tag user.image_url %></td>
    </tr>
    <% end %>
  </table>
  </div>
</div>

